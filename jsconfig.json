{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["components/*"],
      "@/pages/*": ["pages/*"],
      "@/lib/*": ["lib/*"],
      "@/styles/*": ["styles/*"],
      "content-collections": ["./.content-collections/generated"]
    },
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/shared/icons/github.",
    "components/shared/icons/expanding-arrow.js",
    "components/layout/waitlist-modal.js",
    "components/layout/user-dropdown.js",
    "components/layout/sign-in-modal.js",
    "components/layout/meta.js",
    "pages/index.js",
    "components/layout/index.js",
    "components/home/web-vitals.js",
    "components/home/faq.js",
    "components/home/demo-modal.js",
    "components/home/card.js",
    "pages/_document.js",
    "pages/_app.js",
    "pages/login/google/auth.js",
    "pages/dashboard/index.js",
    "pages/api/og.js",
    "lib/utils/index.js",
    "lib/api/user.js",
    "lib/api/useItem.js",
    "lib/storage.js",
    "lib/constants.js",
    "lib/axios.js",
    "lib/auth.js",
    "lib/api/api.js",
    "lib/hooks/use-window-size.js",
    "lib/hooks/use-scroll.js",
    "lib/hooks/use-local-storage.js",
    "lib/hooks/use-intersection-observer.js",
    "components/shared/validate-email.js",
    "components/shared/tooltip.js",
    "components/shared/toggle.js",
    "components/shared/table.js",
    "components/shared/subheader.js",
    "components/shared/popover.js",
    "components/shared/modal.ks",
    "components/shared/leaflet.js",
    "components/shared/footer.js",
    "components/shared/counting-numbers.js",
    "components/shared/icons/twitter.js",
    "components/shared/icons/loading-spinner.js",
    "components/shared/icons/loading-dots.js",
    "components/shared/icons/loading-circle.js",
    "components/shared/icons/index.js",
    "components/shared/icons/google.js",
    "@/components/magicui/text-shimmer.jsx",
    "@/components/magicui/sphere-mask.jsx",
    "@/components/magicui/particles.jsx",
    "@/components/magicui/marquee.jsx",
    "@/components/magicui/border-beam.jsx",
    "pages/blog/index.js",
    "pages/blog/[slug]/page.js",
    "lib/docs-source.js",
    "content-collections.js"
  ],
  "exclude": ["node_modules"]
}